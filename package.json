{
  "name": "confidant",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "confidant:dev": "ng serve",
    "confidant:build": "ng build --configuration=production",
    "confidant:build-desktop": "ng build --configuration=desktop --base-href ./",
    "confidant:build-desktop-high-mem": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --configuration=desktop --base-href ./",
    "desktop:dev": "electron desktop/main.js",
    "desktop:local": "electron-builder",
    "desktop:devLocal": "electron-builder -c.compression=store",
    "desktop:full": "electron-builder -wml --x64"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@types/jest": "^24.9.0",
    "@types/mocha": "^5.2.7",
    "bignumber.js": "^9.0.0",
    "bip39": "^3.0.2",
    "blakejs": "^1.1.0",
    "bootstrap": "^4.4.1",
    "buffer": "^5.4.3",
    "concat-typed-array": "^1.0.2",
    "electron-log": "^4.0.3",
    "libsodium-wrappers": "^0.7.6",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
